{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [true, { "limit": 200 }],
    "interface-name": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": false,
    "ordered-imports": false,
    "jsdoc-format": false,
    "no-trailing-spaces": false,
    "no-var-requires": true,
    "no-debugger": false,
    "no-shadowed-variable": true,
    "quotemark": [
      true,
      "single"
    ],
    "no-trailing-whitespace": false,
    "semicolon": [
      false,
      "always"
    ],
    "trailing-comma": [
      false,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "one-line": [
      false,
      "check-open-brace",
      "check-catch",
      "check-else",
      "check-whitespace"
    ],
    "no-console": false
  }
}